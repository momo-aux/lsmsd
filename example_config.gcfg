; This is an example config for the lsms server.

[Network]
ListenTo = ":8080"
[Crypto]
Enabled = false
Certificate = "./certfile.pem"
KeyFile = "./keyfile.key"
Pepper "./.pepper"
[Database]
Server = "localhost"
Prefix = "lsmsd_"
[Mail]
Enabled = false
StartTLS = true
ServerAddress = ""
Username = ""
Password = ""
EMailAddress = ""
Admins = ""
[Logging]
Level = "Info"
