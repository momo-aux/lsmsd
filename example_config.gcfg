; This is an example config for the lsms server.

[Network]
ListenTo = ":8080"
[Crypto]
Enabled = false
Certificate = "./certfile.pem"
KeyFile = "./keyfile.key"
Pepperfile = "./.pepper"
[Database]
Server = "localhost"
DB = "lsmsd_"
[Mail]
Enabled = false
StartTLS = true
ServerAddress = ""
Username = ""
Password = ""
EMailAddress = ""
[Logging]
Level = "Info"
